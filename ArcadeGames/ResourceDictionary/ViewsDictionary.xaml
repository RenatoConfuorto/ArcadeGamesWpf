<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:ViewModels="clr-namespace:ArcadeGames.ViewModels"
                    xmlns:Views="clr-namespace:ArcadeGames.Views"
                    xmlns:TrisViewModels="clr-namespace:Tris.ViewModels;assembly=Tris"
                    xmlns:TrisViews="clr-namespace:Tris.Views;assembly=Tris"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <DataTemplate DataType="{x:Type ViewModels:HomeViewModel}">
        <Views:HomeView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type TrisViewModels:TrisHomePageViewModel}">
        <TrisViews:TrisHomePageView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type TrisViewModels:TrisMultiplayerViewModel}">
        <TrisViews:TrisMultiplayer/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type TrisViewModels:TrisSingleplayerViewModel}">
        <TrisViews:TrisSingleplayer/>
    </DataTemplate>
</ResourceDictionary>